<script>
    $(document).ready(function() {
      $("#searchbtn").click(search)
    });

    function search() {

      var a = document.getElementById('search_barID').value;
      console.log(a);

      $.get('/bookstore', {"name": a});
      // $.post('/bookstore', )
    }
