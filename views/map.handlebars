<!DOCTYPE HTML>
<html>
<head>
  <title>Map</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css"
   integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ=="
   crossorigin=""/>

  <link rel="stylesheet" type="text/css" href="/css/map.css">
  <!-- <link rel="stylesheet" type="text/css" href="/css/index.css"> -->
  <script defer src="https://use.fontawesome.com/releases/v5.0.6/js/all.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
</head>
<body>
    <div class="container-fluid" id="naviContainer">
    <div class="row no-gutters" id="topheader">
        <div class="col-xs-6" id="history">
                <a class="navbar-brand" onclick="goBack()"><i class="fas fa-angle-left"></i></a>
    <script type="text/javascript">
      function goBack() {
        window.history.back();
      }
    </script>
        </div>
        <div class="col-xs-6" id="hamburger">
            	<!-- navbar -->
		<nav class="navbar navbar-light navbar-1 white">
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent15" aria-controls="navbarSupportedContent15"
			aria-expanded="false" aria-label="Toggle navigation"><span class="navbar-toggler-icon"></span></button>
                    			<div class="collapse navbar-collapse" id="navbarSupportedContent15">
				<ul class="navbar-nav mr-auto">
					<li class="nav-item active">
						<a class="nav-link" href="/page_A">Home <span class="sr-only">(current)</span></a>
					</li>
					<li class="nav-item active">
						<a class="nav-link" href="/userprofile">Profile</a>
					</li>
					<li class="nav-item active">
						<a class="nav-link" href="/help">Help</a>
					</li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/addstore">Add Store</a>
                    </li>
				</ul>
			</div>
		</nav>
        </div>
    </div>
    </div>

  <div id="image-map"></div>
  
  <div class="container-fluid boxofcheckboxes">
    <div class="row">
      <div class="col-4 border rounded">
        Elevators
      </div>
      <div class="col-2">
        <input class="firstCheck" type="checkbox">
      </div>
      <div class="col-4 border rounded">
        Hazard
      </div>
      <div class="col-2">
        <input class="fifthCheck" type="checkbox">
      </div>
    </div>

    <div class="row">
      <div class="col-4 border rounded">
        Escalator Down
      </div>
      <div class="col-2">
        <input class="secondCheck" type="checkbox">
      </div>
      <div class="col-4 border rounded">
        Restroom
      </div>
      <div class="col-2">
        <input class="sixthCheck" type="checkbox">  
      </div>
    </div>

    <div class="row">
      <div class="col-4 border rounded">
        Escalator Up
      </div>
      <div class="col-2">
        <input class="thirdCheck" type="checkbox">
      </div>
      <div class="col-4 border rounded">
        Stairs
      </div>
      <div class="col-2">
        <input class="seventhCheck" type="checkbox">
      </div>
    </div>

    <div class="row">
      <div class="col-4 border rounded">
        Closure
      </div>
      <div class="col-2">
        <input class="fourthCheck" type="checkbox">
      </div>
      <div class="col-4 border rounded">
        Checkout
      </div>
      <div class="col-2">
        <input class="eighthCheck" type="checkbox">  
      </div>
    </div>
    {{#each data}}
    <form method="get" action="/addToMap">
      <input id="hiddenAddress" type="hidden" name="hiddenAddress" value="{{address}}">
      <input id="hiddenMapImage" type="hidden" name="hiddenMapImage" value="{{mapImage}}">
      <button type="submit" class="btn-outline-dark">+</button>
    </form>
    {{/each}}
  </div>
  
  
  
  
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  <script src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js"
   integrity="sha512-/Nsx9X4HebavoBvEBuyp3I7od5tA0UzAxs+j83KgC8PU0kgB4XiK4Lfe4y4cgBtaRJQEIFCW+oC506aPT2L1zw=="
   crossorigin=""></script>
  <script src="/js/map.js"></script>
</body>
</html>